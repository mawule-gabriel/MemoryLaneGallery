{
  "family": "memorylane-task",
  "executionRoleArn": "arn:aws:iam::ACCOUNT_ID:role/memorylane-ecs-execution-role",
  "taskRoleArn": "arn:aws:iam::ACCOUNT_ID:role/memorylane-ecs-task-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "memorylane-container",
      "image": "ACCOUNT_ID.dkr.ecr.us-east-1.amazonaws.com/memorylane-repo:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "S3_BUCKET_NAME",
          "value": "memorybucket10"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/memorylane",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
